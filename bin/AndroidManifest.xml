<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tao.scfestimer"
    android:versionCode="13"
    android:versionName="2.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="19" />
    <uses-permission android:name="android.permission.VIBRATE"/>
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher_winter"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" android:screenOrientation="portrait" android:launchMode="singleTask">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity><receiver android:name="Notifier"></receiver>
        
        <receiver android:name="Notifier02"></receiver>
        <receiver android:name="Notifier03"></receiver>
        <receiver android:name="Notifier04"></receiver><activity android:name="MenuActivity" android:label="@string/setting" android:screenOrientation="portrait" android:launchMode="singleTask"></activity>
        <activity android:name="FAQ" android:label="@string/FAQ" android:screenOrientation="portrait"></activity>
        <activity android:name="Update" android:label="@string/update" android:screenOrientation="portrait"></activity>
        <activity android:name="HideFunction" android:label="@string/HideFunction" android:screenOrientation="portrait"></activity>
        <activity android:name="Coop" android:screenOrientation="portrait" android:label="@string/coop"></activity>
        
    </application>

</manifest>
